{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "commonjs",
    "lib": ["dom", "es2022"],
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": ".erb/dll",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./",
    "paths": {
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@models": ["src/models"],
      "@models/*": ["src/models/*"],
      "@navigation": ["src/renderer/navigation"],
      "@navigation/*": ["src/renderer/navigation/*"],
      "@screens": ["src/renderer/screens"],
      "@screens/*": ["src/renderer/screens/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"]
    }
  },
  "exclude": ["test", "release/build", "release/app/dist", ".erb/dll"]
}
